PROGRAM AUTOMOVEL;
USES CRT;
VAR
    NOME            : STRING [ 15];
    PFABRICA,PFINAL : REAL;
BEGIN
     CLRSCR;
     WRITELN ('ESCREVA NOME DO AUTOMOVEL E SEU PRE€O DE FABRICA');
     READLN (NOME);
     READLN (PFABRICA);
     PFINAL := ( PFABRICA * 0.73 ) + PFABRICA;
     WRITELN ('O AUTOMOVEL ',NOME,' CUSTA ',PFINAL);
     REPEAT UNTIL KEYPRESSED
END.