PROGRAM CALCULO_DA_HIPOTENUSA;
USES CRT;
VAR
    CATETO1,CATETO2,SOMA,HIPOTENUSA : REAL;
BEGIN
     CLRSCR;
     WRITELN('DIGITE O VALOR DO CATETO 1');
     READLN (CATETO1);
     WRITELN('DIGITE O VALOR DO CATETO 2');
     READLN (CATETO2);
     SOMA := SQR(CATETO1) + SQR(CATETO2);
     HIPOTENUSA :=SQRT(SOMA);
     WRITELN ('HIPOTENUSA = ',HIPOTENUSA);
END.