PROGRAM EXERCICIO_6;
USES CRT;
VAR
   STRING_NUMERICA            : STRING;
   NUMERO_INTEIRO,ERRO        : INTEGER;
   TECLA                      : CHAR;
BEGIN
     CLRSCR;
     WRITELN ('DIGITE UMA STRING NUMERICA');
     READLN (STRING_NUMERICA);
     VAL (STRING_NUMERICA,NUMERO_INTEIRO,ERRO);
     WRITELN ('ESSE VALOR AGORA E UM NUMERO INTEIRO : ',NUMERO_INTEIRO);
     TECLA := READKEY
END.